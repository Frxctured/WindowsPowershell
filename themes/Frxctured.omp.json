{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "pink": "#ff97c2",
        "purple": "#C3A0E8",
        "lilac": "#EBE1F8",
        "white": "#FFFFFF",
        "text": "#5e4646",
        "success": "#BDEFA8",
        "error": "#F09696",
        "warning": "#FFD580",
        "blue": "#89CFF0",
        "green": "#98D8C8"
    },
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "p:purple",
                    "foreground": "p:text",
                    "powerline_symbol": "\ue0b4",
                    "leading_diamond": "\ue0b6",
                    "style": "diamond",
                    "template": "{{.Icon}} ",
                    "type": "os"
                },
                {
                    "background": "p:purple",
                    "foreground": "p:text",
                    "powerline_symbol": "\ue0b4",
                    "style": "diamond",
                    "template": "@ajan.zuberi",
                    "type": "session"
                },
                {
                    "background": "p:pink",
                    "foreground": "p:text",
                    "properties": {
                        "mapped_locations": {
                            "C:\\Users\\yu007666": "~"
                        },
                        "folder_icon": "\uf07c",
                        "folder_separator_icon": " \ue0b1 ",
                        "home_icon": "\uf015",
                        "style": "agnoster_short",
                        "max_depth": 3
                    },
                    "powerline_symbol": "\ue0b4",
                    "style": "powerline",
                    "template": " {{ .Path }}",
                    "type": "path"
                },
                {
                    "background": "p:lilac",
                    "foreground": "p:text",
                    "style": "powerline",
                    "properties": {
                        "branch_icon": "\ue725 ",
                        "cherry_pick_icon": "\ue29b ",
                        "commit_icon": "\uf417 ",
                        "fetch_status": false,
                        "fetch_upstream_icon": true,
                        "merge_icon": "\ue727 ",
                        "no_commits_icon": "\uf0c3 ",
                        "rebase_icon": "\ue728 ",
                        "revert_icon": "\uf0e2 ",
                        "tag_icon": "\uf412 "
                    },
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}p:warning{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:error{{ end }}",
                        "{{ if gt .Ahead 0 }}p:blue{{ end }}"
                    ],
                    "powerline_symbol": "\ue0b4",
                    "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "type": "shell",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b4",
                    "leading_powerline_symbol": "\ue0b6",
                    "background": "p:lilac",
                    "foreground": "p:text",
                    "template": "\uf489 {{ .Name }}"
                },
                {
                    "type": "time",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b4",
                    "background": "p:pink",
                    "foreground": "p:text",
                    "template": " {{ .CurrentDate | date \"15:04\" }}",
                    "properties": {
                        "time_format": "15:04"
                    }
                },
                {
                    "foreground": "p:text",
                    "background": "p:pink",
                    "powerline_symbol": "\ue0b4",
                    "type": "executiontime",
                    "style": "powerline",
                    "template": " {{ if eq .Code 0 }}\uf43a{{ else }}\uf52f{{ end }} {{ .FormattedMs }}",
                    "properties": {
                        "threshold": 500,
                        "style": "austin"
                    },
                    "background_templates": [
                        "{{ if eq .Code 0 }}p:success{{ end }}",
                        "{{ if ne .Code 0 }}p:error{{ end }}"
                    ]
                },
                {
                    "background": "p:purple",
                    "foreground": "p:text",
                    "powerline_symbol": "\ue0b4",
                    "style": "powerline",
                    "type": "python",
                    "properties": {
                        "display_mode": "context",
                        "fetch_virtual_env": true,
                        "fetch_version": true
                    },
                    "template": " \ue235 "
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:purple",
                    "style": "plain",
                    "template": " \u2570\u2500\uf101 ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 3
}
